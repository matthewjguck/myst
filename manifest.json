{
  "manifest_version": 3,
  "name": "Myst",
  "version": "1.0",
  "description": "Demystify your browsing experience",
  "permissions": [
    "tabs",
    "storage",
    "sidePanel",
    "activeTab"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Open Screenshot Capture"
  },
  "side_panel": {
    "default_path": "sidepanel.html"
  }
}